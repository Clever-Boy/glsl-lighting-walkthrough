{
  "name": "glsl-lighting-walkthrough",
  "version": "1.0.0",
  "description": "an example of shading in GLSL with glslify",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "budo index.js:bundle.js --live --verbose -- -t babelify -t glslify -t [ installify --save ] -p errorify | garnish",
