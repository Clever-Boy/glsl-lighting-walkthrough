    "async-each": "^0.1.6",
    "bunny": "^1.0.1",
    "canvas-fit": "^1.4.0",
    "canvas-fit-loop": "^1.0.2",
    "gl-basic-shader": "^1.3.0",
    "gl-geometry": "^1.0.3",
    "gl-mat4": "^1.1.3",
    "gl-shader": "^4.0.1",
    "gl-texture2d": "^2.0.8",
    "gl-vec3": "^1.0.3",
    "glsl-diffuse-lambert": "^1.0.0",
    "glsl-face-normal": "^1.0.2",
    "glsl-inverse": "^1.0.0",
    "glsl-perturb-normal": "^1.0.2",
    "glsl-specular-blinn-phong": "^1.0.0",
    "glsl-transpose": "^1.0.0",
